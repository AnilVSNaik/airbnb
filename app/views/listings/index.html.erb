<p id="notice"><%= notice %></p>

<h1>Listings</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Desc</th>
      <th>Amenities</th>
      <th>House rules</th>
      <th>Reviews</th>
      <th>Price</th>
      <th>Host</th>
      <th>Location</th>
      <th>Similar listings</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @listings.each do |listing| %>
      <tr>
        <td><%= listing.name %></td>
        <td><%= listing.desc %></td>
        <td><%= listing.amenities %></td>
        <td><%= listing.house_rules %></td>
        <td><%= listing.reviews %></td>
        <td><%= listing.price %></td>
        <td><%= listing.host %></td>
        <td><%= listing.location %></td>
        <td><%= listing.similar_listings %></td>
        <td><%= link_to 'Show', listing %></td>
        <td><%= link_to 'Edit', edit_listing_path(listing) %></td>
        <td><%= link_to 'Destroy', listing, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Listing', new_listing_path %>
